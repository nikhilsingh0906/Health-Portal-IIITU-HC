  <div class="container" id="Contact">
    <h2 class="text-center">Contact US</h2>
    <div class="row mt-4 mb-4">
      <div class="col-md-6 offset-md-3">
        <form action="/contactUS" method="POST">
            <input type="text" class="form-control" name="name" placeholder="Name" pattern="[a-zA-Z\s]+" oninvalid="setCustomValidity('Plz enter only Alphabets and Spaces only')" onchange="try{setCustomValidity('')}catch(e){}" required ><br>
            <input type="text" class="form-control" name="subject" placeholder="Subject" required><br>
            <input type="email" class="form-control" name="email" placeholder="E-mail"pattern="[a-zA-Z0-9.!#$%&’+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\.[a-zA-Z0-9-]+)" title="Enter a valid E-mail address, For e.g. John@gmail.com" required><br>
            <textarea class="form-control" name="message" placeholder="How can we help you?" style="height:150px;" required></textarea><br>
            <input class="btn btn-primary" type="submit" value="Send" name="submit"><br><br>
           </form>
      </div>
    </div>
